@using RAPC9Y_SOF_2023241.MVC.Models
@model IEnumerable<Champion>

<h2 class="adminTitle">Admin Menu</h2>

<div class="adminDiv">
    
    @foreach (var item in Model.OrderBy(t=>t.Name))
    {
        <div class="adminChamp">
            <p>@item.Name</p>
            <p>@item.Gender</p>
            <p>@item.Resources</p>
            <p>@item.Region.RegionName</p>
            <p>@item.Release</p>
            <a href="/champion/update?id=@item.Id" class="">Update</a>
            <a href="/champion/delete?id=@item.Id" class="">Delete</a>
        </div>
    }
</div>
